<% if skipped_entries.any? %>

<div class="box">
<h3>Skipped Time Entries</h3>
    
    <table class="list">
    <thead>
          <tr>
            <th>Project</th>
            <th>Date</th>
            <th></th>
          </tr>
    </thead>
    <% for skipped_entry in skipped_entries %>
      <tr title="<%=h skipped_entry.skipped_text %>" >
        <td><%=h skipped_entry.summary %></td>
        <td><%=h skipped_entry.start_date.strftime('%m/%d/%y %I:%M:%S %p %Z') %></td>
        <td><%= link_to 'See Details', skipped_entry %></td>
      </tr>
    <% end %>
    
    </table>
</div>

<% end %>
