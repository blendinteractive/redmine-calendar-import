<h1>Listing user_to_project_mappings</h1>

<table>
  <tr>
    <th>User</th>
    <th>Project</th>
    <th>Project alias</th>
    <th>Event GUID</th>
  </tr>

<% for user_to_project_mapping in @user_to_project_mappings %>
  <tr>
    <td><%=h user_to_project_mapping.user_id %></td>
    <td><%=h user_to_project_mapping.project_id %></td>
    <td><%=h user_to_project_mapping.project_alias %></td>
    <td><%=h user_to_project_mapping.event_guid %></td>
    <td><%= link_to 'Show', user_to_project_mapping %></td>
    <td><%= link_to 'Edit', edit_user_to_project_mapping_path(user_to_project_mapping) %></td>
    <td><%= link_to 'Destroy', user_to_project_mapping, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

